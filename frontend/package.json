{
  "name": "domain-tools",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"bsb -make-world -w\" \"postcss public/tailwind.css -o public/build/tailwind.css --watch\"  \"live-server ./public\"",
    "bsb:build": "bsb -make-world",
    "bsb:clean": "bsb -clean-world",
    "development": "cross-env NODE_ENV=development postcss public/tailwind.css -o public/build/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss public/tailwind.css -o public/build/tailwind.css",
    "start": "yarn run serve",
    "test": "jest --watchAll --env=node"
  },
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "reason-react": "0.8.0",
    "tailwindcss": "^1.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "@glennsl/bs-jest": "^0.4.9",
    "bs-platform": "^7.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "gentype": "^3.22.0",
    "live-server": "^1.2.1",
    "postcss-cli": "^6.1.2"
  }
}
